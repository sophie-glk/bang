{
   "name": "Bang!",
   "version": "2.8",
   "browser_specific_settings": {
      "gecko": {
         "id": "bangs@tt7753",
         "strict_min_version": "109.0"
      }
   },
   "options_ui": {
      "page": "options.html"
   },
   "description": "An extension to add duckduckgo bangs to various search engines",
   "background": {
      "scripts": [
         "background.js",
         "bang.js",
         "update.js",
         "shared_functions.js",
         "grant_permissions.js"
      ]
   },
   "content_scripts": [
      {
         "matches": [
            "https://*.startpage.com/*"
         ],
         "js": [
            "search_engine.js",
            "startpage.js",
            "shared_functions.js"
         ]
      }
   ],
   "permissions": [
      "webRequest",
      "tabs",
      "storage",
      "webRequestBlocking"
      ],
